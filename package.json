		"html-webpack-tags-plugin": "^3.0.2",
		"jest": "^29.7.0",
		"postcss-prefix-selector": "^2.1.1",
		"react-refresh": "^0.18.0",
		"serve-handler": "^6.1.6",
		"webpack": "^5.103.0",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@types/eslint": "^9.6.1",
		"@types/jest": "^29.5.14",
		"@types/mock-fs": "^4.13.4",
		"@types/node": "^20.19.26",
		"@types/semver": "^7.7.1",
		"@types/serve-handler": "^6.1.4",
		"chalk": "^4.1.2",
		"commander": "^13.1.0",
		"esbuild": "^0.27.1",
		"fast-glob": "^3.3.3",
		"fs-extra": "^11.3.2",
		"husky": "^9.1.7",
