		"prettier": "^3.7.4",
		"prettier-plugin-packagejson": "^2.5.20",
		"pretty-quick": "^4.2.2",
		"shx": "^0.4.0",
		"type-fest": "^4.41.0",
		"typescript": "^5.9.3"
