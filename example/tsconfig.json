{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"checkJs": false,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"isolatedModules": true,
		"jsx": "react",
		"lib": ["DOM", "ESNext"],
		"noEmit": true,
		"skipLibCheck": true,
